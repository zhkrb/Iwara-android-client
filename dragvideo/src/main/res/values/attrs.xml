    </declare-styleable>
    <declare-styleable name="VideoPlayerView">
        <attr name="appear_animation" format="reference"/>
    </declare-styleable>
    <declare-styleable name="CutCardView">
        <attr name="AllCut" format="boolean"/>
        <attr name="TopLeftCut" format="boolean"/>
        <attr name="TopRightCut" format="boolean"/>
